{
    "require" : {
        "php": ">=5.3.0",
        "raveren/kint" : "*",
        "laravel/framework" : "*",
        "j4mie/idiorm" : "*",
        "mustache/mustache" : "*",
        "silex/silex": "~1.0",
        "silex/web-profiler": "~1.0",
        "slim/slim" : "*",
        "slim/extras" : "*",
        "phpunit/phpunit": "3.7.*",
        "symfony/dependency-injection" : "*"
    },
    "require-dev": {
        "way/generators": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ],
        "psr-0" : {
            "Arx" : "_vendor/arx/core/src/"
        },
        "files" : [
            "_vendor/arx/core/src/Arx/core.php"
        ]
    },
    "minimum-stability": "dev"
}